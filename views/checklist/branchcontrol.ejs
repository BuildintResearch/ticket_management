                              <label for="atm_id_dropdown" class="label">Intime</label>
                              <input name="intime" for="atm-code" id="atm_id_dropdown" type="text" class="input" placeholder="">
                            </div>
                          </div>

                          <div class="col-6">
                            <div class="group">
                              <label for="atm_id_dropdown" class="label">Out Time</label>
                              <input name="outtime" for="atm-code" id="atm_id_dropdown" type="text" class="input" placeholder="">
                            </div>
                          </div>
                          
