                                <input name="currdate" for="dateTime" class="form-control" type="text" id="datetime" placeholder="Select Date"/>
                              <script type="text/javascript">
                                var dateNow = new Date();
                                $('#datetime').datetimepicker({
                                    format: 'DD-MM-YYYY HH:mm',
                                    sideBySide:true,
                                    useCurrent: true,
                                    locale: 'en',
                                    defaultDate: dateNow
                                });
                      
                              </script>
                            </div>
                          </div>
